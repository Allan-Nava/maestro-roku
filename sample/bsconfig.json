{
  "stagingFolderPath": "build",
  "rootDir": "src",
  "files": [
    "manifest",
    "source/**/*.*",
    "images/**/*.*",
    "sounds/**/*.*",
    "sounds/*.*",
    "fonts/**/*.*",
    "meta/**/*.*",
    "components/**/*.*"
  ],
  "autoImportComponentScript": true,
  "createPackage": false,
  "diagnosticFilters": [
    {
      "src": "**/roku_modules/**/*.*"
    },
    {
      "src": "**/RALETrackerTask.*"
    }
  ],
  "plugins": ["maestro-roku-bsc-plugin", "rooibos-roku", "roku-log-bsc-plugin"],
  "maestro": {
    "excludeFilters": ["**/roku_modules/**/*", "**/rooibos/**/*"],
    "buildForIDE": true
  },
  "rooibos": {
    "isRecordingCodeCoverage": false,
    "testsFilePattern": null
  },
  "rokuLog": {
    "strip": false,
    "insertPkgPath": true,
    "removeComments": true
  },
  "logLevel": "error",
  "retainStagingFolder": true
}
